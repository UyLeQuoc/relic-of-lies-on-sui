{
	"name": "relic-of-lies",
	"private": true,
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev",
		"lint": "turbo run lint",
		"check-types": "turbo run check-types",
		"format": "biome check .",
		"format:fix": "biome check . --write",
		"codegen": "npx sui-ts-codegen generate && node ./scripts/fix-codegen-imports.js"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.10",
		"prettier": "^3.7.4",
		"turbo": "^2.7.2",
		"typescript": "5.9.2"
	},
	"engines": {
		"node": ">=18"
	},
	"packageManager": "bun@1.2.23",
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"dependencies": {
		"@mysten/codegen": "^0.5.13"
	}
}
